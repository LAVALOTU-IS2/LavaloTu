 <p id='notice'><%= notice %></p>
<div class="wrapper container">
  <h1>Lista de Usuarios</h1>
  <table>
    <tr>
      <th class="main">Users</th>
      <th class="main">Email</th>
      <th class="main">Role</th>
      <th class="main" colspan="3"></th>
    </tr>

    <% @users.each do |user| %>
    <tr class="content">
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.role %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
    <% end %>
  </table>
  <br>
</div>


