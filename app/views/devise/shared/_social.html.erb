<%- if devise_mapping.omniauthable? %>
  <div class="row">
  	<%- resource_class.omniauth_providers.each do |provider| %>
	    <div class="social col-md-6 <%= OmniAuth::Utils.camelize(provider).underscore %>">
	    	<%= link_to "", omniauth_authorize_path(resource_name, provider), :class => "fa fa-#{OmniAuth::Utils.camelize(provider).underscore}" %>
	    </div>
	<% end -%>
  </div>
<% end -%>